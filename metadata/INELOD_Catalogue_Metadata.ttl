# -----------------------------
# Theme taxonomy
# -----------------------------
pubsec:demografia
        a               skos:Concept ;
        skos:inScheme   <http://datos.gob.es/kos/sector-publico/sector> ;
        skos:prefLabel  "Demografía"@es , "Demography"@en .

pubsec:empleo
        a               skos:Concept ;
        skos:inScheme   <http://datos.gob.es/kos/sector-publico/sector> ;
        skos:prefLabel  "Empleo"@es, "Employment"@en .

pubsec:education
        a               skos:Concept ;
        skos:inScheme   <http://datos.gob.es/kos/sector-publico/sector> ;
        skos:prefLabel  "Educación"@es, "Education"@en .

pubsec:economia
        a               skos:Concept ;
        skos:inScheme   <http://datos.gob.es/kos/sector-publico/sector> ;
        skos:prefLabel  "Economía"@es, "Economy"@en .

pubsec:turismo
        a               skos:Concept ;
        skos:inScheme   <http://datos.gob.es/kos/sector-publico/sector> ;
        skos:prefLabel  "Turismo"@es, "Tourism"@en .

pubsec:inmigracion-emigracion
        a               skos:Concept ;
        skos:inScheme   <http://datos.gob.es/kos/sector-publico/sector> ;
        skos:prefLabel  "Inmigración y Emigración"@es, "Immigration and Emigration"@en ;
        skos:broader pubsec:demografia.


pubsec:padron
        a               skos:Concept ;
        skos:inScheme   <http://datos.gob.es/kos/sector-publico/sector> ;
        skos:prefLabel  "Padrón"@es, "Census"@en;
        skos:broader pubsec:demografia.


pubsec:formacion
        a               skos:Concept ;
        skos:inScheme   <http://datos.gob.es/kos/sector-publico/sector> ;
        skos:prefLabel  "Formacion"@es;
        skos:broader pubsec:educacion.

pubsec:trabajo
        a               skos:Concept ;
        skos:inScheme   <http://datos.gob.es/kos/sector-publico/sector> ;
        skos:prefLabel  "Trabajo"@es;
        skos:broader pubsec:empleo.

pubsec:mercado-laboral
        a               skos:Concept ;
        skos:inScheme   <http://datos.gob.es/kos/sector-publico/sector> ;
        skos:prefLabel  "Mercado Laboral"@es;
        skos:broader pubsec:empleo.

pubsec:alojamientos
        a               skos:Concept ;
        skos:inScheme   <http://datos.gob.es/kos/sector-publico/sector> ;
        skos:prefLabel  "Alojamientos"@es;
        skos:broader pubsec:turismo.

pubsec:hosteleria
        a               skos:Concept ;
        skos:inScheme   <http://datos.gob.es/kos/sector-publico/sector> ;
        skos:prefLabel  "Hostelería"@es;
        skos:broader pubsec:turismo.


